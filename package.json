    "lodash-es": "4.17.21",
    "mitt": "3.0.1",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-browserify": "1.0.1",
    "path-to-regexp": "6.2.1",
    "pinia": "2.1.7",
    "screenfull": "6.0.2",
    "vue": "3.3.8",
    "vue-router": "4.2.5",
    "vxe-table": "4.4.1",
    "vxe-table-plugin-element": "3.0.7",
    "xe-utils": "3.5.11"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.58",
    "@types/js-cookie": "3.0.6",
